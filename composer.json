{
    "name": "shino47/laravel-yappy-checkout",
    "description": "Integración para Laravel del Botón de Pago Yappy",
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "BancoGeneral\\YappyCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BancoGeneral\\YappyCheckout\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BancoGeneral\\YappyCheckout\\YappyCheckoutServiceProvider"
            ],
            "aliases": {
                "YappyCheckout": "BancoGeneral\\YappyCheckout\\Facades\\YappyCheckoutFacade"
            }
        }
    }
}
